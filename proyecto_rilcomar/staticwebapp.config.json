{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/rilcomar/*"]
  },
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
